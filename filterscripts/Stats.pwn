CMD:identidade(playerid, params[]){
	new file[128];
	new msg[10000];
	new hab[128],habar[128],habm[128],vip[128],porte[128],lincencaM[128],licencaAR[128],licencaWater[128];
	format(file, sizeof(file), CONTAS, PlayerName(playerid));
	if(DOF2_GetInt(file, "HabTerrestre") == 1){
		format(hab, sizeof(hab), "Sim");
	}else if(DOF2_GetInt(file, "HabTerrestre") == 0){
		format(hab, sizeof(hab), "Nao");
	}

	if(DOF2_GetInt(file, "HabAerea") == 1){
		format(habar, sizeof(habar), "Sim");
	}else if(DOF2_GetInt(file, "HabAerea") == 0){
		format(habar, sizeof(habar), "Nao");
	}

	if(DOF2_GetInt(file, "HabAquatica") == 1){
		format(habm, sizeof(habm), "Sim");
	}else if(DOF2_GetInt(file, "HabAquatica") == 0){
		format(habm, sizeof(habm), "Nao");
	}

	if(DOF2_GetInt(file, "IsVip") == 1){
		format(vip, sizeof(vip), "Sim");
	}else if(DOF2_GetInt(file, "IsVip") == 0){
		format(vip, sizeof(vip), "Nao");
	}


	if(DOF2_GetInt(file, "Porte de Armas") == 1){
		format(porte, sizeof(porte), "Sim");
	}else if(DOF2_GetInt(file, "Porte de Armas") == 0){
		format(porte, sizeof(porte), "Nao");
	}

	if(DOF2_GetInt(file, "Exercito") == 1){
		format(lincencaM, sizeof(lincencaM), "Sim");
	}else if(DOF2_GetInt(file, "Exercito") == 0){
		format(lincencaM, sizeof(lincencaM), "Nao");
	}

	if(DOF2_GetInt(file, "Marinha") == 1){
		format(licencaWater, sizeof(licencaWater), "Sim");
	}else if(DOF2_GetInt(file, "Marinha") == 0){
		format(licencaWater, sizeof(licencaWater), "Nao");
	}

	if(DOF2_GetInt(file, "Aeronautica") == 1){
		format(licencaAR, sizeof(licencaAR), "Sim");
	}else if(DOF2_GetInt(file, "Aeronautica") == 0){
		format(licencaAR, sizeof(licencaAR), "Nao");
	}
	new str[1000]
	format(msg, sizeof(msg), "{00FF00}Nome: {FFFFFF}%s\n", PlayerName(playerid));
	strcat(str, msg);
	format(msg, sizeof(msg), "{00FF00}Level: {FFFFFF}%d\n",GetPlayerScore(playerid));
	strcat(str, msg);
	format(msg, sizeof(msg), "{00FF00}Exp: {FFFFFF}%d\n", DOF2_GetInt(file, "Exp"));
	strcat(str, msg);
	format(msg, sizeof(msg), "{00FF00}Nivel de procurado: {FFFFFF}%d\n", DOF2_GetInt(file, "Nivel de procurado"));
	strcat(str, msg);
	format(msg, sizeof(msg), "{00FF00}Email: {FFFFFF}%s\n", DOF2_GetString(file, "Email"));
	strcat(str, msg);
	format(msg, sizeof(msg), "{00FF00}Skin ID: {FFFFFF}%d\n", DOF2_GetInt(file, "Skin"));
	strcat(str, msg);
	format(msg, sizeof(msg), "{00FF00}Dinheiro: {FFFFFF}$%d\n", GetPlayerMoney(playerid));
	strcat(str, msg);
	format(msg, sizeof(msg), "{00FF00}Habilitacao Terrestre: {FFFFFF}%s\n", hab);
	strcat(str, msg);
	format(msg, sizeof(msg), "{00FF00}Habilitacao Aerea: {FFFFFF}%s\n", habar);
	strcat(str, msg);
	format(msg, sizeof(msg), "{00FF00}Habilitacao Maritima: {FFFFFF}%s\n", habm);
	strcat(str, msg);
	format(msg, sizeof(msg), "{00FF00}Emprego: {FFFFFF}%s\n", DOF2_GetString(file,"Emprego"));
