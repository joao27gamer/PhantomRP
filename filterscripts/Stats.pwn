	format(msg, sizeof(msg), "{00FF00}Porte de armas: {FFFFFF}%s\n", porte);
	strcat(str, msg);
	format(msg, sizeof(msg), "{00FF00}Licenca do Exercito: {FFFFFF}%s\n", lincencaM);
