forward LvlUP(playerid);
public LvlUP(playerid){
	new file[128];
	format(file, sizeof(file), "contas/%s.ini", PlayerName(playerid));
	if(Experiencia[playerid] < 6){
		new s1[128];
		Experiencia[playerid]++;
		format(s1, sizeof(s1), "{00FF00}[UP] {FFFFFF}Voce recebeu 1 de Exp. (%d/6)", Experiencia[playerid]);
		DOF2_SetInt(file, "Exp", Experiencia[playerid])
		SendClientMessage(playerid, -1, s1);
	}else{
		new s1[128];
		Experiencia[playerid] = 0;
		SetPlayerScore(playerid, GetPlayerScore(playerid) +1);
		DOF2_SetInt(file, "Exp", Experiencia[playerid]);
		DOF2_SetInt(file, "Lrvel", Experiencia[playerid]);
		
		format(s1, sizeof(s1), "{00FF00}[UP] {FFFFFF}Voce juntou 6 de Exp e alcanÃ§ou o nivel %d.", GetPlayerScore(playerid));
		SendClientMessage(playerid, -1, s1);
	}
	
	return 1;
}

